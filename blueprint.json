{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"preferredVersions": {
		"php": "8.2",
		"wp": "latest"
	},
	"features": {
		"networking": true
	},
	"extraLibraries": [
		"wp-cli"
	],
	"plugins": [
		"https://github-proxy.com/proxy/?repo=soderlind/super-admin-all-sites-menu"
	],
	"steps": [
		{
			"step": "login",
			"username": "admin"
		},
		{
			"step": "enableMultisite"
		},
		{
			"step": "wp-cli",
			"command": "wp site create --slug='site-01' --title='Test Site 01'"
		},
		{
			"step": "wp-cli",
			"command": "wp site create --slug='site-02' --title='Test Site 02'"
		},
		{
			"step": "wp-cli",
			"command": "wp site create --slug='site-03' --title='Test Site 03'"
		}
	]
}